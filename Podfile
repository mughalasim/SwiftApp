  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftTestApp

  # Rest calls
  pod 'Alamofire', '~> 4.9.0'
  
  # fast Json object mapping
  pod 'SwiftyJSON', '~> 4.1'
  
  # Toasts and Dialogs
  pod 'Toast-Swift', '~> 5.0.1'

  
end